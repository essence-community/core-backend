{
    "name": "ungate",
    "version": "3.0.1",
    "description": "Шлюз",
    "private": true,
    "engines": {
        "yarn": ">=1.13.0",
        "node": ">=14.0.0"
    },
    "scripts": {
        "prettier": "prettier --write \"{,!(node_modules|dist|build|bin|libs|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
        "build": "gulp all",
        "clear": "rimraf bin",
        "pre-commit": "yarn prettier && yarn build",
        "tslint:server": "tslint --project server/tsconfig.json",
        "tslint:fix:server": "tslint --project server/tsconfig.json --fix",
        "tslint:plugininf": "tslint --project plugininf/tsconfig.json",
        "tslint:fix:plugininf": "tslint --project plugininf/tsconfig.json --fix",
        "tslint:fix:all": "gulp tslint:fix:all"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn pre-commit"
        }
    },
    "author": "Ivan Artemov (honyrik@gmail.com)",
    "license": "MIT",
    "workspaces": [
        "server",
        "plugininf",
        "contexts/*",
        "plugins/*",
        "providers/*",
        "schedulers/*",
        "events/*",
        "libs/*"
    ],
    "devDependencies": {
        "@essence-community/eslint-config-react": "^2.2.0",
        "@types/async": "^3.2.1",
        "@types/bcrypt-nodejs": "0.0.31",
        "@types/body-parser": "^1.17.0",
        "@types/commander": "^2.12.2",
        "@types/compression": "1.7.2",
        "@types/cpy": "^7.1.3",
        "@types/dotenv": "^8.2.0",
        "@types/errorhandler": "1.5.0",
        "@types/es6-shim": "^0.31.39",
        "@types/glob": "^7.1.4",
        "@types/gulp": "^4.0.5",
        "@types/http-server": "^0.12.1",
        "@types/jest": "^27.0.3",
        "@types/lodash": "^4.14.118",
        "@types/node": "^16.11.11",
        "@types/request": "^2.48.1",
        "@types/rimraf": "^3.0.0",
        "commander": "^8.3.0",
        "cpy": "^8.1.2",
        "eslint": "^7.21.0",
        "glob": "^7.2.0",
        "gulp": "^4.0.0",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^6.0.0-alpha.1",
        "husky": "^7.0.4",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "tslint": "^6.1.2",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.2.3"
    }
}
